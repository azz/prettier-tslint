{
  "root": true,
  "extends": ["eslint:recommended"],
  "plugins": ["prettier"],
  "env": {
    "es6": true
  },
  "globals": {
    "process": true
  },
  "rules": {
    "curly": ["error"],
    "prefer-const": ["error"],
    "prettier/prettier": "error",
    "no-var": ["error"]
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
